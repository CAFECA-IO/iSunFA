  tokenContract          String?       @map("token_contract")
  tokenId                String?       @map("token_id")
  name                   String
  from                   Int
  to                     Int
  type                   String
  reportType             String        @map("report_type")
  status                 String
  remainingSeconds       Int?          @map("remaining_seconds")
