    "jsonwebtoken": "^9.0.2",
    "jspdf": "^3.0.1",
    "lodash": "^4.17.20",
    "lodash-es": "^4.17.21",
    "mjml": "^4.15.3",
    "mustache": "^4.2.0",
    "next": "^14.2.5",
    "next-auth": "^4.24.7",
    "next-i18next": "^15.4.2",
    "next-logger": "^5.0.0",
    "next-qrcode": "^2.5.1",
    "next-session": "^4.0.5",
    "nodemailer": "^6.10.1",
    "pdf-to-img": "^4.4.0",
    "pino": "^9.3.2",
    "pino-multi-stream": "^6.0.0",
    "pino-pretty": "^11.2.2",
    "pusher": "^5.2.0",
    "pusher-js": "^8.4.0-rc2",
    "react": "^18.3.1",
    "react-apexcharts": "^1.6.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.3.1",
    "react-easy-crop": "^5.4.1",
    "react-hotkeys-hook": "^4.6.1",
    "react-i18next": "^15.0.1",
    "react-icons": "^5.0.1",
    "react-markdown": "^9.0.1",
    "react-pdf": "^9.1.0",
    "react-to-pdf": "^1.0.1",
    "react-to-print": "^3.0.2",
    "react-toastify": "^10.0.5",
    "react-usestateref": "^1.0.8",
    "remark-breaks": "^4.0.0",
    "remark-gfm": "^4.0.0",
    "rotating-file-stream": "^3.2.3",
    "sharp": "^0.33.3",
    "supertest": "^7.1.1",
    "tailwind-merge": "^2.2.2",
    "uuid": "^10.0.0",
    "winston": "^3.14.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@babel/eslint-plugin": "^7.25.1",
    "@babel/preset-typescript": "^7.24.1",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^16.0.0",
    "@types/cookie": "^0.6.0",
    "@types/decimal.js": "^0.0.32",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/mjml": "^4.7.4",
    "@types/mustache": "^4.2.5",
    "@types/node": "^22.4.2",
    "@types/nodemailer": "^6.4.15",
    "@types/pino": "^7.0.5",
    "@types/react": "^18.3.4",
    "@types/react-dom": "^18.3.0",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.57.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-next": "^14.2.5",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-tailwindcss": "^3.17.4",
    "husky": "^9.1.5",
    "jest": "^29.7.0",
    "lint-staged": "^16.1.6",
    "postcss": "^8.4.32",
    "prettier": "^3.3.2",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "prisma": "^5.21.1",
    "rimraf": "^6.0.1",
    "supertest": "^7.1.1",
    "tailwindcss": "^3.4.0",
    "ts-jest": "^29.1.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "next lint && eslint --fix --ext .js,.jsx,.ts,.tsx . && npm run test && lint-staged && npm run update-version"
    }
  },
  "engines": {
    "node": ">=20.19.5"
  },
  "overrides": {
    "typescript": "^5.9.2",
    "@types/node": "^22.4.2"
  },
  "prisma": {
    "seed": "ts-node  -r tsconfig-paths/register --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  }
}
